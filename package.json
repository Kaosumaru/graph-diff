{
  "name": "graph-diff",
  "version": "0.0.1",
  "description": "A tool to diff graphs",
  "main": "./out/main/index.js",
  "author": "Mateus Borycki",
  "homepage": "https://github.com/Kaosumaru/graph-diff",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "deep-equal": "^2.2.3",
    "electron-log": "^5.2.0",
    "react-json-view-compare": "^2.0.2",
    "rete": "^2.0.5",
    "rete-area-plugin": "^2.1.1",
    "rete-comment-plugin": "^2.0.2",
    "rete-connection-plugin": "^2.0.5",
    "rete-react-plugin": "^2.0.7",
    "rete-readonly-plugin": "^2.0.2",
    "rete-render-utils": "^2.0.3",
    "ts-command-line-args": "^2.5.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.2",
    "vite": "^5.3.1"
  }
}
